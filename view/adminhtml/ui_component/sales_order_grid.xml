<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="fontis_australia_exporteparcel">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">fontis_australia_exporteparcel</item>
                        <item name="label" xsi:type="string" translate="true">eParcel Export</item>
                        <item name="url" xsi:type="url" path="fontis_australia/eparcel/exportorders"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
